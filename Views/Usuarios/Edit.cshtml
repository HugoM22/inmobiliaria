@model Inmobiliaria1.Models.Usuario
@{
    ViewData["Title"] = "Editar usuario";
    ViewBag.ShowPasswordInput = false;
}
<h2 class="mb-3">Editar usuario</h2>

<form asp-action="Edit" method="post" enctype="multipart/form-data" class="card p-3">
    <input type="hidden" asp-for="Id" />
    <partial name="_Form" model="Model" />
    <div class="text-end">
        <a asp-action="Index" class="btn btn-outline-secondary">Volver</a>
        <button class="btn btn-primary" type="submit">Guardar cambios</button>
    </div>
</form>

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}
